{
    "name": "next-practice",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "cross-env APP_MODE=dev next dev",
        "build": "cross-env APP_MODE=prod next build",
        "start": "cross-env APP_MODE=prod next start",
        "analyze": "npx next experimental-analyze",
        "storybook:dev": "storybook dev -p 6006 -c config/storybook",
        "storybook:build": "storybook build -c config/storybook",
        "lint": "eslint . --config ./config/eslint/eslint.config.mjs",
        "format": "prettier --config ./config/eslint/prettier.config.cjs --write .",
        "format:check": "prettier --config ./config/eslint/prettier.config.cjs --check .",
        "test:unit": "jest --config ./config/jest/jest.config.ts"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^2.11.2",
        "next": "16.1.6",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "react-redux": "^9.2.0"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^5.0.0",
        "@storybook/addon-a11y": "^10.2.6",
        "@storybook/addon-docs": "^10.2.6",
        "@storybook/nextjs-vite": "^10.2.6",
        "@svgr/webpack": "^8.1.0",
        "@swc/plugin-react-remove-properties": "^12.5.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@types/jest": "^30.0.0",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "babel-plugin-react-compiler": "1.0.0",
        "cross-env": "^10.1.0",
        "eslint": "^9",
        "eslint-config-next": "16.1.6",
        "eslint-plugin-jest": "^29.12.2",
        "eslint-plugin-jest-dom": "^5.5.0",
        "eslint-plugin-storybook": "^10.2.6",
        "eslint-plugin-testing-library": "^7.15.4",
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "prettier": "^3.8.1",
        "sass": "^1.97.3",
        "storybook": "^10.2.6",
        "ts-node": "^10.9.2",
        "typescript": "^5",
        "vite": "^7.3.1"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    }
